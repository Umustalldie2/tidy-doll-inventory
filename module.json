{
  "id": "tidy-doll-inventory",
  "title": "Tidy doll-inventory",
  "description": "A module that allow drag and drop inventory management on a 'doll' ; in a way many rpg video games do. It requires tidy5e-sheet and item-pile modules",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": 10,
    "verified": 10.291,
    "maximum": 10
  },
  "authors": [
    {
      "name": "Rwanoux",
      "url": "https://github.com/rwanoux",
      "discord": "rwanoux#3031"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/v10-dev/system.json",
        "compatibility": {
          "minimum": "2.0.0-alpha1",
          "verified": "2.1.5"
        }
      }
    ],
    "requires": [
      {
        "id": "item-piles",
        "compatibility": {
          "minimum": "2.6.13",
          "verified": "2.6.14"
        },
        "type": "module"
      },
      {
        "id": "tidy5e-sheet",
        "compatibility": {
          "minimum": "0.8.27",
          "verified": "0.8.41"
        },
        "type": "module"
      }
    ]
  },
  "esmodules": [
    "scripts/module.js"
  ],
  "styles": [
    "css/doll-inventory.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "media": [
    {
      "type": "icon",
      "url": "https://avatars.githubusercontent.com/u/18176317?v=4"
    }
  ]
}